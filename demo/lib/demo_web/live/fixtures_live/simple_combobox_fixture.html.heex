<form>
  <.combobox id="demo-combobox">
    <.combobox_input
      name="demo-combobox[fruit]"
      placeholder="Type to search..."
    />

    <.combobox_options id="demo-combobox-options">
      <%= for option <- ["Apple", "Pear", "Mango", "Pineapple"] do %>
        <.combobox_option value={option}>
          {option}
        </.combobox_option>
      <% end %>
    </.combobox_options>
  </.combobox>
</form>
